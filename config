# Path to your oh-my-zsh configuration.
ZSH=$HOME/.oh-my-zsh

# Set name of the theme to load.
# Look in ~/.oh-my-zsh/themes/
# Optionally, if you set this to "random", it'll load a random theme each
# time that oh-my-zsh is loaded.
ZSH_THEME="themes/eduardolundgren"

# Which plugins would you like to load? (plugins can be found in ~/.oh-my-zsh/plugins/*)
# Custom plugins may be added to ~/.oh-my-zsh/custom/plugins/
# Example format: plugins=(rails git textmate ruby lighthouse example rationalise-dot)
plugins=(mgr ant ant-ext brew cloudapp extract gem git git-completion git-delete-history git-ext git-flow git-tools github history-substring-search liferay macports mysql node npm osx python rake ruby textmate rationalise-dot z)

#
# Editor
#
export EDITOR='subl -w'

#
# Liferay
#
export ALLOY_UI="$LIFERAY_GIT/alloy-ui";
export LIFERAY_BUNDLE="$LIFERAY_BUNDLES/liferay-portal";
export LIFERAY_PLUGINS="$LIFERAY_GIT/liferay-plugins";
export LIFERAY_PLUGINS_EE="$LIFERAY_GIT/liferay-plugins-ee";
export LIFERAY_PORTAL="$LIFERAY_GIT/liferay-portal";
export LIFERAY_PORTAL_EE="$LIFERAY_GIT/liferay-portal-ee";